{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "operator-linebreak": 0,
    "import/prefer-default-export": 0,
    "indent": ["error", 4],
    "comma-dangle": ["error", "never"],
    "arrow-parens": ["error", "as-needed"],
    "max-len": ["error", 120],
    "no-unused-vars": ["error", { "args": "none" }]
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
